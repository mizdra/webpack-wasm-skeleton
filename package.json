{
  "name": "webpack-wasm-skeleton",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/mizdra/webpack-wasm-skeleton.git",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "prestart": "npm run build",
    "build:wasm": "cargo build --release --target wasm32-unknown-unknown",
    "build:js": "webpack --mode production --config webpack.config.js",
    "build": "run-s build:wasm build:js",
    "dev:wasm": "cargo watch -x 'build --release --target wasm32-unknown-unknown'",
    "dev:js": "webpack-dev-server --mode development --config webpack.config.js",
    "test": "echo TODO",
    "pretest": "npm run build"
  },
  "devDependencies": {
    "html-webpack-plugin": "^3.0.6",
    "npm-run-all": "^4.1.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
}
